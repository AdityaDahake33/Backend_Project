{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.+)",
      "dest": "app.js"
    },
    {
      "src": "/",
      "dest": "app.js"
    }
  ],
  "includeFiles": ["views/**"],
  "functions": {
    "app.js": {
      "includeFiles": "views/**"
    }
  }
}
